{
	"name": "vl-ui-demo",
	"version": "1.1.1",
	"description": "Componenten die gebruikt worden om de demo pagina te construeren.",
	"author": "DIDM",
	"license": "MIT",
	"main": "./src/index.js",
	"exports": {
		"require": "./index.js",
		"import": "./src/index.js",
		"node": "./index.js",
		"default": "./index.js"
	},
	"homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-demo#readme",
	"bugs": {
		"url": "https://github.com/milieuinfo/webcomponent-vl-ui-demo/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-demo.git"
	},
	"publishConfig": {
		"registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
	},
	"scripts": {
		"prepare": "npm update --dev vl-ui-util && npm run prepare:util",
		"prepare:util": "npm explore vl-ui-util -- npm run util",
		"test": "npm run test:unit && npm run test:e2e",
		"test:unit": "wct --config-file ./wct.local.conf.json",
		"test:e2e": "npm run test:server:start && npm run test:e2e:chrome && npm run test:e2e:firefox",
		"test:e2e:grid": "npm run test:server:start && npm run test:e2e:chrome:grid && npm run test:e2e:firefox:grid",
		"test:grid": "wct --config-file ./wct.docker.conf.json && npm run test:e2e:grid",
		"posttest:e2e": "pkill -f http-server",
		"test:e2e:chrome": "mocha --chrome",
		"test:e2e:chrome:grid": "mocha --chrome --grid",
		"test:e2e:firefox": "mocha --firefox",
		"test:e2e:firefox:grid": "mocha --firefox --grid",
		"test:e2e:opera": "mocha --opera",
		"test:e2e:safari": "mocha --safari",
		"test:server:start": "http-server &",
		"release": "np",
		"release:prepare": "npm explore vl-ui-util -- npm run release-component:prepare $(pwd) demo",
		"release:testless": "np --yolo",
		"demo": "http-server"
	},
	"dependencies": {
		"prismjs": "1.20.0",
		"vl-ui-content-header": "^3.0.1",
		"vl-ui-core": "^5.5.1",
		"vl-ui-footer": "^3.1.0",
		"vl-ui-grid": "^3.1.0",
		"vl-ui-header": "^3.1.0",
		"vl-ui-template": "^3.0.2",
		"vl-ui-titles": "^3.0.2"
	},
	"peerDependencies": {
		"vl-ui-core": "^5.5.1",
		"vl-ui-template": "^3.0.1",
		"vl-ui-header": "^3.1.0",
		"vl-ui-footer": "^3.1.0",
		"vl-ui-grid": "^3.0.2",
		"vl-ui-content-header": "^3.0.1",
		"vl-ui-titles": "^3.0.1",
		"prismjs": "1.20.0"
	},
	"devDependencies": {
		"@govflanders/vl-ui-util": "3.9.2",
		"vl-ui-util": "^5.2.2"
	}
}
